	<queryString>
		<![CDATA[select cuenta,sum(debe) as deudor,sum( haber) as acreedor,  
        CASE 
           WHEN (sum(debe)-sum(haber) ) > 0
               THEN (sum(debe)-sum(haber) )
           ELSE 0
       END saldo_deudor,
       CASE 
           WHEN (sum(haber)-sum(debe) ) > 0
               THEN (sum(haber)-sum(debe) )
           ELSE 0 
       END saldo_acreedor
 from asiento
where id_libro = $P{libro_id}
 group by cuenta
order by cuenta asc]]>
	</queryString>
